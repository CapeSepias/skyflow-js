{
  "name": "skyflow-client-sdk",
  "preferGlobal": true,
  "analyze": false,
  "version": "0.1.0",
  "author": "Skyflow",
  "description": "Skyflow JavaScript SDK",
  "main": "./dist/sdkNodeBuild/index.js",
  "types": "./types/index-node.d.ts",
  "keywords": [
    "client",
    "sdk",
    "javascript"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "webpack-dev-server --config=webpack.dev.js --open --hot ",
    "build-browser-sdk": "webpack --config=webpack.skyflowBrowser.js",
    "build-node-sdk": "webpack --config=webpack.skyflowNode.js",
    "build-iframe": "webpack --config=webpack.iframe.js",
    "test": "jest --config=jest.config.json --coverage"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "framebus": "^4.0.0",
    "inject-stylesheet": "^2.0.0",
    "jquery": "^3.5.1",
    "jquery-mask-plugin": "^1.14.16",
    "jss": "^10.7.1",
    "jss-preset-default": "^10.7.1",
    "jwt-decode": "^3.1.2",
    "set-value": "^4.0.0"
  },
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "@types/jest": "^27.0.1",
    "@types/jquery-mask-plugin": "^1.14.3",
    "@types/node": "^14.0.5",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^27.0.6",
    "typescript": "^3.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  }
}
